{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:e2e:chrome": "playwright test --project=chromium-desktop",
    "test:e2e:firefox": "playwright test --project=firefox-desktop",
    "test:e2e:safari": "playwright test --project=webkit-desktop",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:critical": "playwright test --grep @critical",
    "test:e2e:smoke": "playwright test --grep @smoke"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/mongodb": "^4.0.7",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/tailwindcss": "^3.0.11",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^24.1.3",
    "mongodb": "^6.19.0",
    "msw": "^2.2.1",
    "postcss": "^8.5.6",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^1.6.1"
  }
}
